/* 搜索工具栏 */
.weui-search-bar,
.editor_toolbox {
  position: fixed;
  width: 100%;
  z-index: 999;
}

.search_num {
  position: absolute;
  right: 0;
  top: 0;
  padding: 8px;
  font-size: 14px;
  color: gray;
}

.search_triangle {
  display: none;
  width: 20px;
  height: 20px;
  margin-left: 8px;
  background-image: url('data:image/svg+xml;utf8,<svg t="1595855312634" class="icon" viewBox="0 0 1024 1024" version="1.1" xmlns="http://www.w3.org/2000/svg" p-id="4785"><path d="M512.34106768 334.81806719l-403.42507403 403.41955065c-16.9429169 16.9567253-44.44374272 16.9567253-61.38804046 0-16.9567253-16.9429169-16.9567253-44.44374272 0-61.38804048l434.11978468-434.11978467c16.9429169-16.9567253 44.44374272-16.9567253 61.38804047 0l434.11840383 434.11978467c8.47836265 8.4728393 12.71892482 19.58446539 12.71892483 30.69332981s-4.24056217 22.22049053-12.71892483 30.69332983c-16.9429169 16.9567253-44.44374272 16.9567253-61.38804046 0l-403.42645486-403.41955067z" p-id="4786" fill="%238a8a8a"></path></svg>');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.weui-search-bar_focusing .search_triangle {
  display: block;
}

/* 编辑工具栏 */
.editor_toolbox {
  top: 0;
  background-color: var(--weui-BG-0);
  padding: 5px;
  box-sizing: border-box;
}

@font-face {
  font-family: "iconfont";
  src: url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2');
}

.iconfont {
  flex: 1;
  text-align: center;
  font-family: "iconfont" !important;
  font-size: 22px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

